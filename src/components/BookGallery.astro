/* Default styles for both desktop and mobile */
.book {
  position: relative;
  width: 35rem;
  height: 18.875rem;
  left: 30%;
  right: 0%;
  perspective: 39.375rem;
  perspective-origin: center 3.125rem;
  transform: scale(1.2);
  filter: drop-shadow(0rem .625rem .3125rem rgba(0, 0, 0, 0.25));
}

/* Media query for smaller screens (mobile devices) */
@media (max-width: 47.9375rem) {
  .book {
    width: 1rem;
    height:0.75rem;
    left: 48%;
    transform: none;
    filter: none;
    /* You can add any other mobile-specific styles here */
  }
}

/* Media query for larger screens (desktop devices) */
@media (min-width: 48rem) {
  .book {
    width: 20rem; /* Adjust width for desktop */
    height: 20.5rem; /* Adjust height for desktop */
    left: 37%; /* Adjust position for desktop */
    transform: scale(1.2); /* Keep the scaling for desktop */
    filter: drop-shadow(0rem .625rem .3125rem rgba(0, 0, 0, 0.25)); /* Keep the drop shadow for desktop */
    /* You can add any other desktop-specific styles here */
  }
}


.page {
    width: 13.125rem;
    height: 18.75rem;
    background-color: #bbb;
    position: absolute;
    top: 0rem;
    right: 50%;
    transform-origin: 100% 100%;
    border: solid #555 .125rem;
    background-size: 26.25rem 18.75rem;
    background-position: center;
    transform-style: preserve-3d;
}

.page:nth-child(1) {
    transform: rotateX(60deg) rotateY(3deg);
}

.page:nth-child(2) {
    transform: rotateX(60deg) rotateY(4.5deg);
}

.page:nth-child(3) {
    transform: rotateX(60deg) rotateY(6deg);
    -webkit-animation: nextPage 25s infinite -24s steps(1);
    animation: nextPage 25s infinite -24s steps(1);
    background-size: 26.25rem 18.75rem;
    background-position: -0.125rem -0.125rem;
}

.page:nth-child(4) {
    transform: rotateX(60deg) rotateY(177deg);
}

.page:nth-child(5) {
    transform: rotateX(60deg) rotateY(175.5deg);
}

.page:nth-child(6) {
    transform: rotateX(60deg) rotateY(174deg);
    overflow: hidden;
}

.page:nth-child(6)::after {
    content: "";
    width: 13.125rem;
    height: 18.75rem;
    position: absolute;
    top: 0rem;
    right: 0%;
    transform-origin: center;
    transform: rotateY(180deg);
    -webkit-animation: nextPage 25s -20s infinite steps(1);
    animation: nextPage 25s -20s infinite steps(1);
    background-size: 26.25rem 18.75rem;
    background-position: 100% -0.125rem;
}

@-webkit-keyframes nextPage {
    0% {
      background-image: url("https://raw.githubusercontent.com/follow-prince/project-images/main/IMG-1.jpg");
    }

    20% {
      background-image: url("https://raw.githubusercontent.com/follow-prince/project-images/main/IMG-2.jpg");
    }

    40% {
      background-image: url("https://raw.githubusercontent.com/follow-prince/project-images/main/IMG-3.jpg");
    }

    60% {
      background-image: url("https://raw.githubusercontent.com/follow-prince/project-images/main/IMG-4.jpg");
    }

    80% {
      background-image: url("https://raw.githubusercontent.com/follow-prince/project-images/main/IMG-5.jpg");
    }
}

@keyframes nextPage {
    0% {
        background-image: url("https://raw.githubusercontent.com/follow-prince/project-images/main/IMG-1.jpg");
    }

    20% {
